@model PhoneBook.Models.PhoneBookNode
@{
    ViewData["Title"] = "Добавление пользователя";
}
<form asp-action="Add" asp-controller="PhoneBook">
    <p>Suname <input type="text" asp-for="Suname" /></p>
    <p>Name <input type="text" asp-for="Name" /></p>
    <p>Patronymic <input type="text" asp-for="Patronymic" /></p>
    <p>PhoneNumber <input type="text" asp-for="PhoneNumber" /></p>
    <p>Adress <input type="text" asp-for="Adress" /></p>
    <p>Description <input type="text" asp-for="Description" /></p>
    <p><input type="submit" value="Добавить" /></p>
</form>
